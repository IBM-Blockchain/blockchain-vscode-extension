{
  "homepage": ".",
  "name": "ibm-blockchain-platform-website",
  "description": "End to end extension for Hyperledger Fabric developers. Develop and test your blockchain smart contracts and client applications on your local machine, and package your projects for deployment into IBM Blockchain Platform runtimes.",
  "version": "2.0.9",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM-Blockchain/blockchain-vscode-extension"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=5.6"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bootstrap": "^4.4.1",
    "bootstrap-scss": "^4.4.1",
    "carbon-components": "^10.7.2",
    "carbon-components-react": "^7.7.2",
    "carbon-icons": "^7.0.7",
    "node-sass": "^7.0.1",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.3.2",
    "react-scripts": "^5.0.0"
  },
  "scripts": {
    "unit": "npm test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@carbon/grid": "^10.6.0",
    "@carbon/themes": "^10.7.0",
    "@carbon/type": "^10.6.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^7.2.1",
    "@types/carbon-components-react": "^7.6.2",
    "@types/chai": "^4.3.0",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^25.1.4",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^16.9.0",
    "@types/sinon-chai": "^3.2.8",
    "chai": "4.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gh-pages": "^2.2.0",
    "react-test-renderer": "^16.13.0",
    "sinon": "6.1.4",
    "sinon-chai": "3.2.0",
    "ts-jest": "^27.1.3",
    "typescript": "3.6.3",
    "vscode-test": "1.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/App.tsx",
      "src/components/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
