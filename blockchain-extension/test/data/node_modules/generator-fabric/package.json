{
    "name": "generator-fabric",
    "version": "0.0.7",
    "description": "Yeoman generator for Hyperledger Fabric",
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "files": [
        "generators"
    ],
    "keywords": [
        "yeoman-generator"
    ],
    "author": "Simon Stone",
    "license": "Apache-2.0",
    "dependencies": {
        "yeoman-generator": "^2.0.5"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "eslint": "^4.19.1",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2",
        "yeoman-assert": "^3.1.1",
        "yeoman-test": "^1.7.2"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "generators/**/templates/**",
            "test/**",
            "tmp/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    },
    "contractLanguages": [
        "JavaScript",
        "TypeScript"
    ]
}